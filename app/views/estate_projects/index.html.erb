<p id="notice"><%= notice %></p>

<h1><%= t('views.estate_project.index.title') %></h1>

<%= link_to t('views.estate_project.index.new'), new_estate_project_path %>

<table>
  <thead>
    <tr>
      <th><%= EstateProject.human_attribute_name(:id) %></th>
      <th><%= EstateProject.human_attribute_name(:name) %></th>
      <th><%= EstateProject.human_attribute_name(:created_at) %></th>
      <th><%= EstateProject.human_attribute_name(:updated_at) %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estate_projects.each do |estate_project| %>
      <tr>
        <td><%= estate_project.id %></td>
        <td><%= estate_project.name %></td>
        <td><%= estate_project.created_at %></td>
        <td><%= estate_project.updated_at %></td>
        <td><%= link_to t('views.show'), estate_project %></td>
        <td><%= link_to t('views.edit'), edit_estate_project_path(estate_project) %></td>
        <td><%= link_to t('views.destroy'), estate_project, method: :delete, data: { confirm: t('views.are_you_sure') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
