<p id="notice"><%= notice %></p>

<h1><%= t('views.estate_type.index.title') %></h1>

<%= link_to t('views.estate_type.index.new'), new_estate_type_path %>

<table>
  <thead>
    <tr>
      <th><%= EstateType.human_attribute_name(:id) %></th>
      <th><%= EstateType.human_attribute_name(:name) %></th>
      <th><%= EstateType.human_attribute_name(:created_at) %></th>
      <th><%= EstateType.human_attribute_name(:updated_at) %></th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @estate_types.each do |estate_type| %>
      <tr>
        <td><%= estate_type.id %></td>
        <td><%= estate_type.name %></td>
        <td><%= estate_type.created_at %></td>
        <td><%= estate_type.updated_at %></td>
        <td><%= link_to t('views.show'), estate_type %></td>
        <td><%= link_to t('views.edit'), edit_estate_type_path(estate_type) %></td>
        <td><%= link_to t('views.destroy'), estate_type, method: :delete, data: { confirm: t('views.are_you_sure') } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
